<Window x:Class="SogetiSpain.DI_IoC.ViewLayer.EmployeeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SogetiSpain.DI_IoC.ViewLayer"
        mc:Ignorable="d"
        local:ExecuteMethodOnLoadBehavior.MethodNameToExecute="LoadEmployees"
        Title="Ejemplo Acoplamiento Débil" Height="300" Width="500">
    <Grid>
        <DataGrid ItemsSource="{Binding Employees}" />
    </Grid>
</Window>
